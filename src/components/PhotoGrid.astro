---
import type { FullCarouselImage } from "./FullCarousel";
import PhotoWithModal from "./PhotoWithModal.astro";
const imagePaths: FullCarouselImage[] = Astro.props.imagePaths;
---

<div class="md:mx-24 grid grid-cols-1 md:grid-cols-2 gap-8 mb-48">
  {
    imagePaths.map((image, index) => {
      console.log(image);
      return (
        <PhotoWithModal id={index} imageSrc={image.url} alt={image.title} />
      );
    })
  }
</div>
